package com.zacx.gateway.driver.dto.lines;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;
import lombok.Data;

/**
 * Created by song on 2018/10/5.
 */
@Data
@ApiModel(value = "SchedulingExtResult", description = "排班响应类")
public class SchedulingExtResult extends SchedulingResult {
    @ApiModelProperty("距离")
    private Double distance;//距离
    @ApiModelProperty("起点地址")
    private String siteStartAddr;//起点地址
    @ApiModelProperty("起点名称")
    private String siteStartName;//起点名称
    @ApiModelProperty("终点地址")
    private String siteEndAddr;//终点地址
    @ApiModelProperty("终点名称")
    private String siteEndName;//终点名称
    @ApiModelProperty("起点ID")
    private Integer siteStartId;//起点
    @ApiModelProperty("终点ID")
    private Integer siteEndId;//终点
}
