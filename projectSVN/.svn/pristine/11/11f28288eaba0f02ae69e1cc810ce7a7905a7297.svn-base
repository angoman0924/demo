package com.zacx.serivce.user.api.dto;

import io.swagger.annotations.ApiModel;
import io.swagger.annotations.ApiModelProperty;
import lombok.Data;

@Data
@ApiModel(value = "PassengerDTO", description = "乘客基本信息")
public class PassengerDTO {

    @ApiModelProperty("乘客编号")
    private Integer id;

    @ApiModelProperty("乘客性名")
    private String name;

    @ApiModelProperty("证件类型")
    private Integer cardType;

    @ApiModelProperty("证件号码")
    private String cardNumber;

    @ApiModelProperty("关联用户编号")
    private Long userId;

    @ApiModelProperty("乘客类型")
    private Integer personGroup;

    @ApiModelProperty("联系方式")
    private String mobile;

    @ApiModelProperty("状态 1可用 0不可用")
    private Integer status;


}
